<Window x:Class="DeepSpaceNetwork.LaunchSpacecraft"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DeepSpaceNetwork"
        mc:Ignorable="d"
        Title="LaunchSpacecraft" Height="500" Width="800">
    <Grid Background="AntiqueWhite">
        <Button Content="Back" FontSize="10" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="50" Height="25" Click="Go_Back" />
        <Label Content="Launch Spacecraft" FontSize="40" HorizontalAlignment="Center" Margin="0,45,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.071,-3.327" Width="340" Height="60"/>
        <ComboBox x:Name="SpacecraftList" FontSize="20" HorizontalAlignment="Center" Margin="0,173,0,0" VerticalAlignment="Top" Width="332" Height="52"></ComboBox>
        <Button Content="Launch" FontSize="40" HorizontalAlignment="Center" Margin="0,290,0,0" VerticalAlignment="Top" Width="332" Height="63" Click="Launch_Spacecraft"/>
        <!--<TextBox x:Name="pathToVehicle" HorizontalAlignment="Left" Margin="35,242,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="743"/>-->

    </Grid>
</Window>
